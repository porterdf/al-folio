	year = {2011},
	note = {Publisher: American Geophysical Union},
	file = {Geophys. Res. Lett. 2011 Peterson:/Users/dporter/Zotero/storage/6RHEDJEB/Geophys. Res. Lett. 2011 Peterson.pdf:application/pdf},
}

