@article{wagnerRepresentationCapsizingIceberg2017,
	title = {On the representation of capsizing in iceberg models},
	url = {https://arxiv.org/abs/1702.06870},
	doi = {10.1016/j.ocemod.2017.07.003},
	abstract = {Although iceberg models have been used for decades, they have received far more widespread attention in recent years, in part due to efforts to explicitly represent icebergs in climate models. This calls for increased scrutiny of all aspects of typical iceberg models. An important component of iceberg models is the representation of iceberg capsizing, or rolling. Rolling occurs spontaneously when the ratio of iceberg width to height falls below a critical threshold. Here we examine previously proposed representations of this threshold, and we find that there have been crucial errors in the representation of rolling in many modeling studies to date. We correct these errors and identify an accurate model representation of iceberg rolling. Next, we assess how iceberg rolling influences simulation results in a hierarchy of models. Rolling is found to substantially prolong the lifespan of individual icebergs and allow them to drift farther offshore, although it is found to have relatively small impacts on the large-scale freshwater distribution in comprehensive model simulations. The results suggest that accurate representations of iceberg rolling may be of particular importance for operational forecast models of iceberg drift, as well as for regional changes in high-resolution climate model simulations.},
	journal = {Ocean Modelling},
	author = {Wagner, Till J W and Stern, Alon A and Dell, Rebecca W and Eisenman, Ian},
	year = {2017},
	note = {Publisher: Elsevier},
	file = {Ocean Modelling 2017 Wagner:/Users/dporter/Zotero/storage/4G3BMNF7/Ocean Modelling 2017 Wagner.pdf:application/pdf},
}

