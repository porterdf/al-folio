	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL095477},
	pages = {e2021GL095477},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/WHTDAQKY/Coulson et al. - 2021 - The Global Fingerprint of Modern Ice-Mass Loss on .pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/AF6U5FSI/2021GL095477.html:text/html},
}

@article{davisCausesAcceleratingSea2017,
