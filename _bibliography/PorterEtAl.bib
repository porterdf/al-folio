	author = {Matsuoka, Kenichi and Hindmarsh, Richard C. A. and Moholdt, Geir and Bentley, Michael J. and Pritchard, Hamish D. and Brown, Joel and Conway, Howard and Drews, Reinhard and Durand, Gaël and Goldberg, Daniel and Hattermann, Tore and Kingslake, Jonathan and Lenaerts, Jan T. M. and Martín, Carlos and Mulvaney, Robert and Nicholls, Keith W. and Pattyn, Frank and Ross, Neil and Scambos, Ted and Whitehouse, Pippa L.},
	month = nov,
	year = {2015},
	keywords = {Antarctic Ice Sheet, Holocene deglaciation, Ice dome, Pinning point, Sea-level rise},
	pages = {724--745},
	file = {ScienceDirect Full Text PDF:/Users/dporter/Zotero/storage/987F95E5/Matsuoka et al. - 2015 - Antarctic ice rises and rumples Their properties .pdf:application/pdf;ScienceDirect Snapshot:/Users/dporter/Zotero/storage/RLL76DJ7/S0012825215300416.html:text/html},
