	journal = {The Cryosphere},
	author = {Leong, Wei Ji and Horgan, Huw Joseph},
	month = nov,
	year = {2020},
	note = {Publisher: Copernicus GmbH},
	pages = {3687--3705},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/3WQH7KDI/Leong and Horgan - 2020 - DeepBedMap a deep neural network for resolving th.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/QQU9HEUT/2020.html:text/html},
