	keywords = {GRACE, GNET, mass acceleration, NAO, SMB},
	pages = {1934--1939},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/XK6ZTFAU/Bevis et al. - 2019 - Accelerating changes in ice mass within Greenland,.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/VP2VKY4Z/1934.html:text/html},
