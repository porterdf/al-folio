@article{mortensenLocalCoastalWater2018,
	title = {Local {Coastal} {Water} {Masses} {Control} {Heat} {Levels} in a {West} {Greenland} {Tidewater} {Outlet} {Glacier} {Fjord}},
	volume = {123},
