	pages = {1361--1373},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/C3KMRM6S/Fréville et al. - 2014 - Using MODIS land surface temperatures and the Croc.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/NH4FN46I/2014.html:text/html},
}

