	author = {King, Michalea D. and Howat, Ian M. and Candela, Salvatore G. and Noh, Myoung J. and Jeong, Seonsgu and Noël, Brice P. Y. and van den Broeke, Michiel R. and Wouters, Bert and Negrete, Adelaide},
	month = aug,
