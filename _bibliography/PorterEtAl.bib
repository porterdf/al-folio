@article{martinMillennialScaleVulnerability2019,
	title = {Millennial‐{Scale} {Vulnerability} of the {Antarctic} {Ice} {Sheet} to {Regional} {Ice} {Shelf} {Collapse}},
	volume = {46},
	issn = {0094-8276, 1944-8007},
