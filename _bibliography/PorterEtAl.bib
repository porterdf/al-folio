	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2020MS002385},
	keywords = {climate modeling, machine learning, cloud superparameterization, convection parameterizations},
	pages = {e2020MS002385},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/F7F5CBXX/Mooers et al. - 2021 - Assessing the Potential of Deep Learning for Emula.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/MLDXVNUE/2020MS002385.html:text/html},
