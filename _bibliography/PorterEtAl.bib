@article{ashokkumar21stCenturyEstimates2020,
	title = {21st century estimates of mass loss rates from glaciers in the {Gulf} of {Alaska} and {Canadian} {Archipelago} using a {GRACE} constrained glacier model},
	issn = {1994-0416},
	url = {https://tc.copernicus.org/preprints/tc-2019-325/},
	doi = {10.5194/tc-2019-325},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} Ice mass loss rates from glaciers in the Gulf of Alaska and the Canadian Archipelago are expected to increase through the end of century in response to increasing temperatures. Here, we develop a new glacier model constrained by GRACE gravimetry observations for the period between 2002 and 2017. The high temporal and regional spatial resolution of GRACE mass balance estimates allows us to estimate regional glacier sensitivities to atmospheric changes, and account for higher order of glacier dynamics. We use our regionally constrained models to extrapolate future mass loss under different climate emission scenarios. Generally our 21st century sea level estimates are at the high end compared to other studies. We find that the Gulf of Alaska exhibits the highest mass loss rates between \&minus;79 to \&minus;112\&thinsp;Gt\&thinsp;yr$^{\textrm{\&minus;1}}$ between 2006 and 2100 under different scenarios, and displays the highest sensitivity to the specific scenario (RCP 2.6/4.5/8.5). Our estimates for Baffin Island are significantly higher than prior work (\&minus;57 to \&minus;85\&thinsp;Gt\&thinsp;yr$^{\textrm{\&minus;1}}$) and are comparable to projected mass loss rates from the Ellesmere region (\&minus;63 to \&minus;101\&thinsp;Gt\&thinsp;yr$^{\textrm{\&minus;1}}$).{\textless}/p{\textgreater}},
	language = {English},
	urldate = {2022-06-18},
	journal = {The Cryosphere Discussions},
	author = {Ashokkumar, Lavanya and Harig, Christopher},
	month = mar,
	year = {2020},
	note = {Publisher: Copernicus GmbH},
	pages = {1--32},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/AH3FGCG8/Ashokkumar and Harig - 2020 - 21st century estimates of mass loss rates from gla.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/MGFZ6NC2/tc-2019-325.html:text/html},
