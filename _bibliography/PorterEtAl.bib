	urldate = {2021-06-11},
	journal = {Geoscientific Model Development},
	author = {Proksch, M. and Mätzler, C. and Wiesmann, A. and Lemmetyinen, J. and Schwank, M. and Löwe, H. and Schneebeli, M.},
	month = aug,
	year = {2015},
	note = {Publisher: Copernicus GmbH},
	pages = {2611--2626},
	file = {Full Text PDF:/Users/dporter/Zotero/storage/BFNLP99U/Proksch et al. - 2015 - MEMLS3&a Microwave Emission Model of Layered Snow.pdf:application/pdf;Snapshot:/Users/dporter/Zotero/storage/QDTQYAZM/2015.html:text/html},
}

@article{joughinMarineIceFilchner2004,
	title = {Marine ice beneath the {Filchner}–{Ronne} {Ice} {Shelf}, {Antarctica}: a comparison of estimated thickness distributions},
	volume = {39},
	issn = {0260-3055, 1727-5644},
	shorttitle = {Marine ice beneath the {Filchner}–{Ronne} {Ice} {Shelf}, {Antarctica}},
	url = {https://www.cambridge.org/core/journals/annals-of-glaciology/article/marine-ice-beneath-the-filchnerronne-ice-shelf-antarctica-a-comparison-of-estimated-thickness-distributions/B87F84484CA9CD3611E1681EB34175BC},
	doi = {10.3189/172756404781814717},
	abstract = {In an earlier study, melt/freeze rates beneath most of the Filchner–Ronne Ice Shelf, Antarctica, were estimated using an assumption of a steady-state ice shelf applied to a velocity field derived from RADARSAT and ERS-1 interferometric synthetic aperture radar (InSAR) data and an ice-thickness map inferred from ERS-1 satellite radar altimeter data. Here, we use these basal accumulation rates and the InSAR velocity data to estimate the distribution and thickness of marine ice beneath the Filchner–Ronne Ice Shelf. These estimates are compared with a marine-ice thickness map derived from radio-echo sounding (RES) data and airborne radar altimetry. In general, we find close agreement between these estimates. In the few locations where there are significant differences, the discrepancies are largely attributable to artifacts in the radar-altimeter thickness map. With improvements such as a much smaller footprint, the next generation of altimeters should overcome many of these limitations, leading to improved marine-ice accumulation estimates. Overall, the good agreement with the RES data validates the InSAR-based estimates over much of the Ronne Ice Shelf.},
	language = {en},
	urldate = {2021-06-15},
	journal = {Annals of Glaciology},
	author = {Joughin, Ian and Vaughan, David G.},
	year = {2004},
	note = {Publisher: Cambridge University Press},
