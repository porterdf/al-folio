@article{budikovaRoleArcticSea2009,
	title = {Role of {Arctic} sea ice in global atmospheric circulation: {A} review},
	volume = {68},
	doi = {10.1016/j.gloplacha.2009.04.001},
	abstract = {Formed by the freezing of sea water, sea ice defines the character of the marine Arctic. The principal purpose of this review is to synthesize the published efforts that document the potential impact of Arctic sea ice on remote climates. The emphasis is on atmospheric processes and the resulting modifications in surface conditions such as air temperature, precipitation patterns, and storm track behavior at interannual timescales across the middle and low latitudes of the Northern hemisphere during cool months. Addressed also are the theoretical, methodological, and logistical challenges facing the current observational and modeling studies that aim to improve our awareness of the role that Arctic sea ice plays in the definition of global climate. Moving towards an improved understanding of the role that polar sea ice plays in shaping the global climate is a subject of timely importance as the Arctic environment is currently undergoing rapid change with little slowing down forecasted for the future. Published by Elsevier B.V.},
