	number = {20},
	urldate = {2021-06-03},
	journal = {Journal of Climate},
	author = {Gossart, A. and Helsen, S. and Lenaerts, J. T. M. and Broucke, S. Vanden and Lipzig, N. P. M. van and Souverijns, N.},
	month = oct,
	year = {2019},
	note = {Publisher: American Meteorological Society
Section: Journal of Climate},
	pages = {6899--6915},
