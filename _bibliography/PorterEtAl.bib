	year = {2021},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL096215},
	keywords = {bathymetry, seafloor topography, ice-ocean interaction, airborne gravity, Amery Ice Shelf, gravity inversion},
