@article{howatTemporalVariabilitySnow2022,
	title = {Temporal variability in snow accumulation and density at {Summit} {Camp}, {Greenland} ice sheet},
	issn = {0022-1430, 1727-5652},
	url = {https://www.cambridge.org/core/product/identifier/S0022143022000211/type/journal_article},
	doi = {10.1017/jog.2022.21},
	abstract = {Abstract
            
              A 3-year record of weekly snow water equivalent (SWE) accumulation at Summit Camp, central Greenland ice sheet, obtained by direct sampling, is presented. While the overall SWE accumulation of 24.2 cm w.e. a
              −1
              matches long-term ice core estimates, variability increases at shorter timescales. Half of the annual SWE accumulation occurs during a few large events, with the average accumulation rate decreasing 35\% between the first and second halves of the record coinciding with exceptional anticyclonic conditions in the spring and summer of 2019. No seasonality in accumulation is detected. Rather, local accumulation rates appear to be significantly impacted by wind redistribution that obscures temporal patterns in snowfall. Surface snow density is consistent, on average, with previously measured values but does not correlate with near surface temperature or wind speed. Two surface mass balance reanalysis models significantly underestimate accumulation rates at Summit Camp. This is concerning because such models are often used to estimate ice-sheet mass loss.},
	language = {en},
	urldate = {2022-05-23},
	journal = {Journal of Glaciology},
	author = {Howat, Ian M.},
	month = apr,
	year = {2022},
	pages = {1--9},
	file = {temporal-variability-in-snow-accumulation-and-density-at-summit-camp-greenland-ice-sheet.pdf:/Users/dporter/Zotero/storage/Q8ZJCG9S/temporal-variability-in-snow-accumulation-and-density-at-summit-camp-greenland-ice-sheet.pdf:application/pdf},
