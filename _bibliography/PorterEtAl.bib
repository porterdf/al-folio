	note = {Publisher: American Geophysical Union},
	file = {Geophys. Res. Lett. 2010 Zhang:/Users/dporter/Zotero/storage/KTVAZEW7/Geophys. Res. Lett. 2010 Zhang.pdf:application/pdf},
}

