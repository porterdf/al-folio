	urldate = {2022-07-12},
	journal = {Remote Sensing of Environment},
	author = {Tabibi, Sajad and Geremia-Nievinski, Felipe and Francis, Olivier and van Dam, Tonie},
	month = oct,
	year = {2020},
	keywords = {Greenland, Antarctica, GNSS, GPS, Sea level, SNR, GNSS-R, Altimetry, Reflectometry, Tidal harmonics},
	pages = {111959},
	file = {1-s2.0-S0034425720303291-mmc1.docx:/Users/dporter/Zotero/storage/3426WHJM/1-s2.0-S0034425720303291-mmc1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;inventory stations gnss-r sea level - 20210208.xlsx:/Users/dporter/Zotero/storage/6V29VZ37/inventory stations gnss-r sea level - 20210208.xlsx:application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;ScienceDirect Full Text PDF:/Users/dporter/Zotero/storage/RBM4K396/Tabibi et al. - 2020 - Tidal analysis of GNSS reflectometry applied for c.pdf:application/pdf;ScienceDirect Snapshot:/Users/dporter/Zotero/storage/BCX99JCR/S0034425720303291.html:text/html},
