	author = {Balaji, V. and Couvreux, Fleur and Deshayes, Julie and Gautrais, Jacques and Hourdin, Frédéric and Rio, Catherine},
	month = apr,
	year = {2022},
	note = {arXiv:2204.06957 [nlin, physics:physics]
type: article},
	keywords = {Physics - Computational Physics, Physics - Atmospheric and Oceanic Physics, Nonlinear Sciences - Chaotic Dynamics},
	file = {arXiv Fulltext PDF:/Users/dporter/Zotero/storage/8RCIZ87R/Balaji et al. - 2022 - Are GCMs obsolete.pdf:application/pdf;arXiv.org Snapshot:/Users/dporter/Zotero/storage/X2KJGNUF/2204.html:text/html},
