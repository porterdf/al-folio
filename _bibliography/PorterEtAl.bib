	file = {Mattingly et al. - 2020 - Strong Summer Atmospheric Rivers Trigger Greenland.pdf:/Users/dporter/Zotero/storage/5MED24D7/Mattingly et al. - 2020 - Strong Summer Atmospheric Rivers Trigger Greenland.pdf:application/pdf},
